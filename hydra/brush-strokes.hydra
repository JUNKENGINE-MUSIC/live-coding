a.setBins(3)
a.show()

noise(3,0.1)
	.kaleid(5)
	.color(2.83,0.91,0.39)
	//.saturate()
	//.posterize()
.colorama()
	.rotate(0, ({time})=>a.fft[0]*.0001)
	.modulate(o0, () => a.fft[0]*0.1)
	.modulate(osc(25,0.1,0.5).kaleid(0.01)
            .scale(({time})=>Math.sin(time)*.5+1))
	.modulate(
  		shape()
  		.pixelate(50,50)
  		.rotate()
  		.modulate(noise(100,({time}) => Math.sin(time/2)/500))
	)
	.scale(.25)
  	.out(o0)
